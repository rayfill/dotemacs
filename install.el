(cl-eval-when (compile load execute)
  (require 'async))
(async-start
 `(lambda ()
    (setq load-path ',load-path)
    (require 'package-utils)
    (package-refresh-contents)
    (package-utils-upgrade-all t))
 (lambda (result)
   (message "package upgrade processing done.")))
